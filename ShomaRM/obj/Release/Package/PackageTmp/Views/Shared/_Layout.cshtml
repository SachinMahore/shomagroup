                fillLCityList(selected);
            }
        });
        fillMarketSourceDDL();
    });
    var saveProspectForm = function () {
     
        $("#divLoader").show();
        var msg = "";
        var fname = $("#txtFirstName1").val();
        var lname = $("#txtLastName1").val();
        var phone = unformatText($("#txtPhone1").val());
        var email = $("#txtEmail1").val();
        var address = $("#txtAddress").val();
        var message = $("#txtMessage").val();
        var pets = $(".icheckbox_square-yellow").hasClass("checked") ? 1 : 0;
        var pid = $("#hndPID").val();
        var unit = $("#hndUID").val();
        var state = $("#ddlState").val();
        var city = $("#ddlCity").val();
        var visittime = $("#txtMoveDate").val();
        var marketSource = $("#ddlMarketSource1").val();

        if (!fname) {
            msg += "Enter First Name</br>";
        }
        if (!lname) {
            msg += "Enter Last Name</br>";
        }
        if (phone == '') {
            msg += "Enter Phone</br>";
        }
        else {
            if (!validatePhone(phone)) {
                msg += 'Invalid Phone No.<br/>';
            }
        }
        if (email == '') {
            msg += "Enter email Id</br>";
        }
        else {
            if (!validateEmail(email)) {
                msg += "Invalid email address.<br/>"
            }
        }
        if (!visittime) {
            msg += "Enter Visit Date</br>";
        }
        if (msg != "") {
            $("#divLoader").hide();
            $.alert({
                title: '',
                content: msg,
                type: 'red'
            });
            return;
        }
        var model = { FirstName: fname, LastName: lname, PhoneNo: phone, EmailId: email, Address: address, Message: message, HavingPets: pets, UnitID: unit, PropertyID: pid, State: state, City: city, VisitDateTime: visittime, MarketSource: marketSource };
        $.ajax({
            url: "/Prospect/SaveProspectForm/",
            type: "post",
            contentType: "application/json utf-8",
            data: JSON.stringify(model),
            dataType: "JSON",
            success: function (response) {
                $("#divLoader").hide();
                $.alert({
                    title: '',
                    content: "Request Sent.",
                    type: 'blue',
                });
                clearProspectForm();
                $("#popContactForm").modal("hide");
            }
        });
    }

    function clearProspectForm() {
        $("#txtFirstName1").val("");
        $("#txtLastName1").val("");
        $("#txtPhone1").val("");
        $("#txtEmail1").val("");
        $("#txtAddress").val("");
        $("#txtMessage").val("");
        $(".icheckbox_square-yellow").removeClass("checked");
        $("#hndUID").val(0);
        $("#ddlState").val(0);
        $("#ddlCity").val(0);
        $("#txtMoveDate").val("");
    }

    var fillLStateDDL = function () {

        $.ajax({
            url: '/City/FillStateDropDownList',
            method: "post",
            //data: JSON.stringify(param),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                if ($.trim(response.error) != "") {
                    //this.cancelChanges();
                } else {
                    $("#ddlState").empty();
                    $("#ddlState").append("<option value='0'>Select State</option>");
                    $.each(response, function (index, elementValue) {
                        $("#ddlState").append("<option value=" + elementValue.ID + ">" + elementValue.StateName + "</option>");
                    });
                }
            }
        });
    }
    var fillLCityList = function (stateid) {
        var params = { StateID: stateid };
        $.ajax({
            url: '/City/GetCityListbyState',
            method: "post",
            data: JSON.stringify(params),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                if ($.trim(response.error) != "") {
                    //this.cancelChanges();
                } else {
                    $("#ddlCity").empty();
                    $("#ddlCity").append("<option value='0'>Select City</option>");
                    $.each(response, function (index, elementValue) {
                        console.log(response)
                        $("#ddlCity").append("<option value=" + elementValue.ID + ">" + elementValue.CityName + "</option>");
                    });
                }
            }
        });
    }
    var fillMarketSourceDDL = function () {
        $.ajax({
            url: '/Admin/ProspectManagement/GetDdlMarketSourceList',
            method: "post",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {

                $("#ddlMarketSource1").empty();
                $("#ddlMarketSource1").append("<option value='0'>-- Select Market Source --</option>");
                $.each(response.model, function (index, elementValue) {
                    $("#ddlMarketSource1").append("<option value=" + elementValue.AdID + ">" + elementValue.Advertiser + "</option>");
                });

            }
        });
    }

    // Get the modal
    var layoutmodal = document.getElementById("modalMaplayout");
    // Get the button that opens the modal


    // Get the <span> element that closes the modal
    var closeMap = document.getElementById("closemap");

    // When the user clicks the button, open the modal

    // When the user clicks on <span> (x), close the modal
    closeMap.onclick = function () {
        layoutmodal.style.display = "none";
        startcarousel();
    }

    // When the user clicks anywhere outside of the modal, close it
    window.onclick = function (event) {
        if (event.target == layoutmodal) {
            layoutmodal.style.display = "none";
        }
    }


    $(document).ajaxStop(function () {
        $("#divLoader").hide();
    });
    $(document).ajaxComplete(function () {
        $("#divLoader").hide();
    });
    $(document).ajaxStart(function () {
        $("#divLoader").show();
    });
    $(document).ready(function () {
        $("#btnmapfloor,#btnmaplayout,#btnmapIndex,#btnmapcontact").on('click', function (e) {
            $("#modalMaplayout").css("display", "block");
