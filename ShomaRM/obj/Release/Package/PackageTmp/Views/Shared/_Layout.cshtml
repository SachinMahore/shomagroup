<script>
    var ForgetPasswordss = function () {
        var msg = "";
        var email = $("#UserEmail").val();
        console.log(email)
        if (email == "") {
            msg += "Enter Registerd Email ID </br> ";
        }

        if (msg != "") {
            $.alert({
                title: "",
                content: msg,
                type: 'red'
            });
            return;
        }

        var model = {
            Email: email,
        };
        $.ajax({
            url: "/ApplyNow/ForgetPassword/",
            type: "post",
            contentType: "application/json utf-8",
            data: JSON.stringify(model),
            dataType: "JSON",
            success: function (response) {
                var msg = response.model.Message;
                if (msg == "Invalid UserName") {
                    $.alert({
                        title: "",
                        content: "Invalid UserName Please Try With Valid Id",
                        type: 'red'
                    });
                }
                else {
                    $.alert({
                        title: "",
                        content: "Password send to your registered email",
                        type: 'red'
                    });
                }
            }


        });
    };

    var SignIn = function () {
        var msg = "";
        var email = $("#UserEmail").val();
        var pass = $("#UserPassword").val();
        console.log($("#UserPassword").val())
        if (email == "") {
            msg += "Enter Registerd Email ID</br>";
        }
        if (pass == "") {
            msg += "Enter Password</br>";
        }

        if (msg != "") {
            $.alert({
                title: "",
                content: msg,
                type: 'red'
            });
            return;
        }

        var model = {
            UserName: email,
            Password: pass,

        };

        $.ajax({
            url: "/ApplyNow/SignIn",
            type: "post",
            contentType: "application/json utf-8",
            data: JSON.stringify(model),
            dataType: "JSON",
            success: function (response) {
                var result = response.UserData.split("|");
                if (result[0] == "-1") {
                    window.location = "/Home/";
                }
                else if (result[0] != 0) {
                    if (result[1] == "ad") {
                        window.location = "/Admin/AdminHome";
                    }
                    else if (result[1] == "te") {
                        window.location = "/Tenant/Dashboard";
                    }
                    else {
                        window.location = "/ApplyNow/Index/" + result[0];
                    }

                    $("#popSignIn").PopupWindow("close");
                } else {
                    alert("Invalid Email ID / Password.")
                }
            }
        });
    };

    function gotoLogin() {

        window.location = "/Account/Login";
    }
    function gotoLogOff() {

        window.location = "/Account/LogOff";
    }
    function gotoApplyNow() {
        window.location = "/ApplyNow/Index/0";
