    var saveMoveInCheckList = function () {
        var msg = "";
      
        var ProspectID = $("#hndProspectID").val();

        var MoveInDate = $("#txtMoveInDate").val();
        var MoveInTime = $("#ddlMoveInTime").val();
        var MoveInCharges = $("#txtPayment").val();
        var InsuranceDoc = $("#fileUploadPOIShow").text();
        var ElectricityDoc = $("#fileUploadPOEShow").text();
        var IsCheckPO = $("#chkPo").is(":checked") ? "1" : "0";
        var IsCheckATT = $("#chkAt").is(":checked") ? "1" : "0";
        var IsCheckWater = $("#chkWater").is(":checked") ? "1" : "0";

        if (!MoveInDate) {
            msg += "Please fill the Move In Date</br>";
        }
        if (!MoveInCharges) {
            msg += "Please fill the Move In Charges</br>";
        }


        if (msg != "") {
            $.alert({
                title: "",
                content: msg,
                type: 'red'
            })
            $("#divLoader").hide();
            return;
        }

        var model = {
            ProspectID: ProspectID,
            MoveInDate: MoveInDate,
            MoveInTime: MoveInTime,
            MoveInCharges: MoveInCharges,
            IsCheckPO: IsCheckPO,
            IsCheckATT: IsCheckATT,
            IsCheckWater: IsCheckWater,
            InsuranceDoc: InsuranceDoc,
            ElectricityDoc: ElectricityDoc,
        }


        $.ajax({
            url: '/CheckList/SaveMoveInCheckList',
            type: 'post',
            data: JSON.stringify(model),
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            success: function (response) {
                $.alert({
                    title: "",
                    content: response.msg,
                    type: 'red'
                })
                setTimeout(function () {
                    window.location.href = "/Home/Index";
                }, 1500);
               
            }
        });
    };

