    $(document).ready(function () {
        $('.map').maphilight();
        $("#cordi_img").maphilight();
        $("#apartimg_2").maphilight();

        $('#cordi_img').click(function (e) {

            var offset = $(this).offset();
            var X = (e.pageX - offset.left);
            var Y = (e.pageY - offset.top);
        
            $('#txtcord').append(X + ',' + Y + ',');
        });
    });

    $("area").mouseover(function () {
        var noffloor = $("#txtNoOfFloor").val();
        $("#hdnFloorPlan").val("");
        $("#txtcord").remove();
        var cordd = "  <textarea class='form-control'id='txtcord'></textarea>";
        $("#divCord").append(cordd);
        $("#fileUploadFloorDetailsShow1").html("Choose a file &hellip");
        if ($("#ddlFloorList").val() != 0) {
            for (var i = 1; i <= noffloor; i++) {
                if ($("#fa_" + i).length) {
                    {
                        $("#fa_" + i).removeClass('active_mouse').data('maphilight', { alwaysOn: false }).trigger('alwaysOn.maphilight');
                    }
                }
            }
            var cont = $("#ddlFloorList").val();
            if ($("#fa_" + cont).length) {
                $("#fa_" + cont).addClass('active_area').data('maphilight', { alwaysOn: true, fillColor: 'FF6347', strokeColor: 'FF6347', }).trigger('alwaysOn.maphilight');
                $("#txtcord").text($("#fa_" + cont).attr("coords"));
            }
            $("#hdnFloorPlan").val($("#fa_" + cont).data("floorplan"));
            $("#fileUploadFloorDetailsShow1").text($("#fa_" + cont).data("floorplan"));

        }
        else {
            $(this).addClass('active_mouse').data('maphilight', { alwaysOn: true, fillColor: 'FF6347', strokeColor: 'FF6347', }).trigger('alwaysOn.maphilight');
        }
    });
    var resetcords = function () {
        if ($("#ddlFloorList").val()==0) {
            $.alert({
                title: 'Alert!',
                content: "Select Floor No.",
                type: 'red'
            });
            return;
        }
        $("#txtcord").remove();
        var cordd = "  <textarea class='form-control'id='txtcord'></textarea>";
        $("#divCord").append(cordd);
        $("#fa_" + $("#ddlFloorList").val()).attr("coords", "");
        $("#fa_" + $("#ddlFloorList").val()).removeClass('active_mouse').data('maphilight', { alwaysOn: false }).trigger('alwaysOn.maphilight');
    }

    var viewcords = function () {
        if ($("#txtcord").val()) {
            $("#fa_" + $("#ddlFloorList").val()).removeClass('active_mouse').data('maphilight', { alwaysOn: false }).trigger('alwaysOn.maphilight');
            $("#fa_" + $("#ddlFloorList").val()).attr("coords", $("#txtcord").val());
            $("#fa_" + $("#ddlFloorList").val()).addClass('active_area').data('maphilight', { alwaysOn: true, fillColor: 'FF6347', strokeColor: 'FF6347', }).trigger('alwaysOn.maphilight');
        } else {
            $.alert({
                title: 'Alert!',
                content: "Select co-ordinates to view",
                type: 'red'
            });
            return;
        }
    }

    var restorecords = function () {
        if (($("#hdnFloorCords").val().trim())) {
            $("#txtcord").remove();
            var cordd = "  <textarea class='form-control'id='txtcord'></textarea>";
            $("#divCord").append(cordd);
            $("#fa_" + $("#ddlFloorList").val()).attr("coords", $("#hdnFloorCords").val());
            $("#fa_" + $("#ddlFloorList").val()).addClass('active_area').data('maphilight', { alwaysOn: true, fillColor: 'FF6347', strokeColor: 'FF6347', }).trigger('alwaysOn.maphilight');
            $("#txtcord").val($("#hdnFloorCords").val());
        }
    }

    var getCord = function (e, cont) {
        
        var offset = $(cont).offset();
        var X = (e.pageX - offset.left);
        var Y = (e.pageY - offset.top);
        $('#txtcord').append(X + ',' + Y + ',');

    }
</script>
