                                                <div class="col-sm-6">
                                                    <span>LEASE TERM : </span>
                                                </div>
                                                <div class="col-sm-6">
                                                    <span id="lblFNLTerm" style="text-decoration:underline;font-weight:bold;">@Model.LeaseTerm</span>
                                                </div>
                                            </div>
                                            <div class="col-sm-7">
