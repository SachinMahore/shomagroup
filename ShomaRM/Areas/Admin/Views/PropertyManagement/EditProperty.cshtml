        if (aid.toString().indexOf(',') > -1) {
            array = aid.split(',')
        } else {
            array.push(aid);
        }
        $.each(array, function (i) {
            $("#chkAmenity" + array[i]).prop("checked", true);
           // $.uniform.update("#chkAmenity" + array[i]);
            amtArray.push(parseFloat(array[i]));

        });
    }
