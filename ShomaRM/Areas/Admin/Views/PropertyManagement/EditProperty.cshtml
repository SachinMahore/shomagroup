                                                <div class="col-sm-2">
                                                    <span>PHONE : </span>
                                                </div>
                                                <div class="col-sm-3 margin-left-0">
                                                    <span id="lblFNLPhone" style="text-decoration:underline;font-weight:bold;"></span>
                                                </div>
                                                <div class="col-sm-2">
                                                    <span>EMAIL : </span>
                                                </div>
                                                <div class="col-sm-6 margin-left-0">
                                                    <span id="lblFNLEmail" style="text-decoration:underline;font-weight:bold;">@Model.Email</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-12 clear">
