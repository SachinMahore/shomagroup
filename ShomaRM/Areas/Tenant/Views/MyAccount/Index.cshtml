        $("#txtDesiredTimeClubroom").timepicki();
        $("#txtDesiredTimeClubroomFrom").timepicki();
        $("#txtDesiredTimeClubroomTo").timepicki();
    });

    $(function () {


        $("#txtDesiredDateSoccer").datepicker({ autoclose: true, startDate: '+1d' }).on('changeDate', function (e) { $("#txtDesiredDateSoccer").attr("value", $("#txtDesiredDateSoccer").val()) });
        $("#txtDesiredDateClubroom").datepicker({ autoclose: true, startDate: '+1d' }).on('changeDate', function (e) { $("#txtDesiredDateClubroom").attr("value", $("#txtDesiredDateClubroom").val()) });
        $("#txtDesiredDateBBQ").datepicker({ autoclose: true, startDate: '+1d' }).on('changeDate', function (e) { $("#txtDesiredDateBBQ").attr("value", $("#txtDesiredDateBBQ").val()) });
        $("#txtDesiredDatePC").datepicker({ autoclose: true, startDate: '+1d' }).on('changeDate', function (e) { $("#txtDesiredDatePC").attr("value", $("#txtDesiredDatePC").val()) });
        $("#txtDesiredDateYoga").datepicker({ autoclose: true, startDate: '+1d' }).on('changeDate', function (e) { $("#txtDesiredDateYoga").attr("value", $("#txtDesiredDateYoga").val()) });

        $("#txtDesiredDate").on("change", function () {
            var formatedDate = $("#txtDesiredDate").val();
            $("#SelectedDate").html(formatedDate);
        });



        $("#txtDateOfBirth").datepicker();
        $("#txtADateOfBirth").datepicker();
        $("#txtFromDate").datepicker();
        $("#txtToDate").datepicker();
        $("#txtTransFromDate").datepicker();
        $("#txtTransToDate").datepicker();
        $("#txtPrefarredDate").datepicker('setStartDate', new Date());


        var currentTime = new Date()
        var minDate = new Date(currentTime.getFullYear(), currentTime.getMonth() + 1, +1); //one day next before month
        var maxDate = new Date(currentTime.getFullYear(), currentTime.getMonth() + 1, +3); // one day before next month
        $("#txtPayDateR").datepicker({
            startDate: minDate,
            endDate: maxDate
        });


        $("#txtChargeDate").datepicker();
        $("#txtGuestVistStartDate").datepicker({ dateFormat: "MM/dd/yyyy", startDate: new Date() }).on('change', function (ev) {
            $("#txtGuestVistEndDate").val('');
            var startDate = $(this).val();
            var endDate = $('#txtGuestVistStartDate').datepicker('getDate');
            endDate.setDate(endDate.getDate() + 9);

            $("#txtGuestVistEndDate").datepicker('setStartDate', startDate);
            $("#txtGuestVistEndDate").datepicker('setEndDate', endDate);
        });
        $("#ddlTransactionType").val(0);
        $("#ddlTransactionType").trigger('change');

        setTimeout(function () {
            $("#ddlChargeType").val(2);
            $("#ddlChargeType").trigger('change');
        }, 3000);


    });
    function dateRangeAccountHistory() {
        $('#txtFromDate').val('');
        $('#txtToDate').val('');
        $('#popDateRangeAccountHistory').modal('show');
    };
</script>

