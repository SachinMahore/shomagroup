    <script>
        $(function () {


            $("#txtDesiredDate").datepicker({
                format: {
                    /*
                     * Say our UI should display a week ahead,
                     * but textbox should store the actual date.
                     * This is useful if we need UI to select local dates,
                     * but store in UTC
                     */
                    toDisplay: function (date, format, language) {
                        var d = new Date(date);
                        d.setDate(d.getDate() - 7);
                        return d.toDateString();
                    },
                    toValue: function (date, format, language) {
                        var d = new Date(date);
                        d.setDate(d.getDate() + 7);
                        return new Date(d);
                    }
                }
            });
            $("#txtDesiredDate").on("change", function () {
                var formatedDate = $("#txtDesiredDate").val();
                $("#SelectedDate").html(formatedDate);
            });


            $("#txtDateOfBirth").datepicker();
            $("#txtADateOfBirth").datepicker();
            $("#txtFromDate").datepicker();
            $("#txtToDate").datepicker();
            $("#txtTransFromDate").datepicker();
            $("#txtTransToDate").datepicker();
            $("#txtPrefarredDate").datepicker();
            $("#txtChargeDate").datepicker();
            $("#txtGuestVistStartDate").datepicker();
            $("#txtGuestVistEndDate").datepicker();
            $("#ddlTransactionType").val(0);
            $("#ddlTransactionType").trigger('change');

            setTimeout(function () {
                $("#ddlChargeType").val(2);
                $("#ddlChargeType").trigger('change');
            }, 3000);


        });
        function dateRangeAccountHistory() {
            $('#txtFromDate').val('');
            $('#txtToDate').val('');
            $('#popDateRangeAccountHistory').modal('show');
        };
    </script>

