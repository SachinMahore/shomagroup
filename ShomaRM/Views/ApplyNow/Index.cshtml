                }
                else if (isAvalFill == 2) {
                    colorfill = "ffff00";
                }
                $("#" + cont).addClass('active_area').data('maphilight', { alwaysOn: true, fillColor: colorfill, strokeColor: colorfill }).trigger('alwaysOn.maphilight');
            }
        });
