                color = "0af11c";
            }
            else if (isAval == 2) {
                color = "ffff00";
            }
            $(this).addClass('active_mouse').data('maphilight', { alwaysOn: true, fillColor: color, strokeColor: color }).trigger('alwaysOn.maphilight');
