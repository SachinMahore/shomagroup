<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Shoma Group | RMTenant</title>
    <meta name="description" content="Shoma is Residential Management Web Portal">
    <meta name="author" content="Shoma">
    <meta name="keyword" content="Shoma Residential Management">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="icon" href="favicon.ico" type="image/x-icon">

    <link rel="stylesheet" href="@Url.Content("/content/assets/css/normalize.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/font-awesome.min.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/fontello.css")">
    <link href="@Url.Content("/content/assets/fonts/icon-7-stroke/css/pe-icon-7-stroke.css")" rel="stylesheet">
    <link href="@Url.Content("/content/assets/fonts/icon-7-stroke/css/helper.css")" rel="stylesheet">
    <link href="@Url.Content("/content/assets/css/animate.css")" rel="stylesheet" media="screen">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/bootstrap-select.min.css")">
    <link rel="stylesheet" href="@Url.Content("/content/bootstrap/css/bootstrap.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/datepicker.css")" />
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/icheck.min_all.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/price-range.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/owl.carousel.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/owl.theme.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/owl.transitions.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/lightslider.min.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/jquery.slitslider.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/wizard.css")">
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/style.css")">
    <link rel="stylesheet" href="@Url.Content("/Content/jquery-confirm-master/css/jquery-confirm.css")" />
    <link rel="stylesheet" href="@Url.Content("/content/assets/css/popupwindow.css")">

    <script src="@Url.Content("~/Content/Pages/common.js")"></script>

    <link rel="stylesheet" href="@Url.Content("/content/assets/css/responsive.css")">
    <script src="@Url.Content("/content/assets/js/jquery-1.10.2.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/modernizr-2.6.2.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/popupwindow.js")"></script>
    <script src="@Url.Content("/content/assets/js/jquery.simplePagination.js")"></script>
    @RenderSection("scripts", required: false)
</head>
<body>
    <div id="preloader">
        <div id="status">&nbsp;</div>
    </div>
    <!-- Body content -->

    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navigation">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="~/Home/Index"><img src="~/content/assets/img/logoTenant.png" alt=""></a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse yamm" id="navigation">
                <div class="button navbar-right">
                    @ShomaRM.Models.ShomaGroupWebSession.CurrentUser.Username
                    <button class="navbar-btn nav-button wow bounceInRight login" onclick="gotoLogin()" data-wow-delay="0.4s">Logout</button>

                </div>
                <ul class="main-nav nav navbar-nav navbar-right">

                    <li class="wow fadeInDown active" data-wow-delay="0.1s"><a class="" href="~/Tenant/TenantHome">Home</a></li>


                    <li class="wow fadeInDown " data-wow-delay="0.1s"><a class="" href="~/Tenant/MyAccount/Index/@ShomaRM.Models.ShomaGroupWebSession.CurrentUser.TenantID">My Profile </a></li>

                    <li class="dropdown ymm-sw " data-wow-delay="0.1s">
                        <a href="#" class="dropdown-toggle " data-toggle="dropdown" data-hover="dropdown" data-delay="200">Booking / Billing <b class="caret"></b></a>
                        <ul class="dropdown-menu navbar-nav">

                            <li>
                                <a href="/Tenant/UtilityBilling">Utility Billing</a>
                            </li>
                            <li>
                                <a href="/Tenant/FacilityBooking/Edit/0">Facility Booking</a>
                            </li>
                            <li>
                                <a href="/Tenant/EventBooking/Edit/0">Event Booking</a>
                            </li>

                            <li>
                                <a href="/Tenant/WorkOrder/">WorkOrder</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->

        </div><!-- /.container-fluid -->
    </nav>
    <div class="row"><br /><br /><br /></div>
    <!-- End of nav bar -->

    @RenderBody()
    <div id="divLoader" style="display:none"></div>
    <!-- Footer area-->
    <div class="footer-area navbar-fixed-bottom">

        <div class="footer-copy text-center">
            <div class="container">
                <div class="row">
                    <div class="pull-left">
                        <span> (C) <a href="http://www.shomagroup.com">Shoma Group</a> , All rights reserved @DateTime.Now.ToString("yyyy")   </span>
                    </div>
                    <div class="bottom-menu pull-right">
                        <ul>
                            <li><a class="wow fadeInUp animated" href="~/Home/Index" data-wow-delay="0.2s">Home</a></li>
                            <li><a class="wow fadeInUp animated" href="~/Property/Index" data-wow-delay="0.3s">Property</a></li>
                            <li><a class="wow fadeInUp animated" href="~/Home/FAQ" data-wow-delay="0.4s">Faq</a></li>
                            <li><a class="wow fadeInUp animated" href="~/Home/Contact" data-wow-delay="0.6s">Contact</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="@Url.Content("/content/bootstrap/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/bootstrap-datepicker.js")"></script>
    <script src="@Url.Content("/content/assets/js/bootstrap-select.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/bootstrap-hover-dropdown.js")"></script>
    <script src="@Url.Content("/content/assets/js/easypiechart.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/jquery.easypiechart.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/owl.carousel.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/wow.js")"></script>
    <script src="@Url.Content("/content/assets/js/icheck.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/price-range.js")"></script>
    <script src="@Url.Content("/content/assets/js/lightslider.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/jquery.ba-cond.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/jquery.slitslider.js")"></script>
    <script src="@Url.Content("/content/assets/js/jquery.bootstrap.wizard.js")"></script>
    <script src="@Url.Content("/content/assets/js/jquery.validate.min.js")"></script>
    <script src="@Url.Content("/content/assets/js/wizard.js")"></script>
    <script src="@Url.Content("/content/assets/js/main.js")"></script>
    <script src="@Url.Content("/Content/jquery-confirm-master/js/jquery-confirm.js")"></script>
</body>
</html>
<style>
    #divLoader {
        position: fixed;
        width: 100%;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        /*background: url(../../Content/assets/images/giphy.gif) center no-repeat rgba(255,255,255,0.5);*/
        background: url(/Content/assets/img/loader.gif) center no-repeat rgba(255,255,255,0.5);
        display: none;
    }
</style>
<script type="text/javascript">
    function gotoLogin() {
        window.location = "/Account/Login";
    }
</script>